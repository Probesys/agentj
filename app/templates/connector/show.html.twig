{% extends 'base.html.twig' %}

{% block title %}Connector{% endblock %}

{% block body %}
    <h1>Connector</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ connector.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ connector.name }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ connector.active ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_connector_index') }}">back to list</a>

    <a href="{{ path('app_connector_edit', {'id': connector.id}) }}">edit</a>

    {{ include('connector/_delete_form.html.twig') }}
{% endblock %}
